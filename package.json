{
  "name": "code-sign-app",
  "version": "0.1.0",
  "private": true,
  "main": "main",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbadm/code-sign-app.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "run-p build:*",
    "build:react": "react-scripts build",
    "build:main": "tsc -p tsconfig.main.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "fix": "prettier-tslint fix 'src/**/*.ts{,x}'",
    "lint": "prettier-tslint check 'src/**/*.ts{,x}'",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.electron.${name}",
    "productName": "КЗ База данных турнира",
    "extraMetadata": {
      "main": "main/index.js"
    },
    "files": [
      {
        "from": ".",
        "to": ".",
        "filter": [
          "build",
          "main"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "publish": {
      "provider": "github",
      "owner": "alexbadm"
    }
  },
  "files": [
    "build",
    "main"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "electron": "^5.0.1",
    "electron-builder": "^20.43.0",
    "faker": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "prettier-tslint": "^0.4.2",
    "react": "^16.8.6",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
